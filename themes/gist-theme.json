{
	"name": "Gist Color Theme",
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
        "activityBar.background": "#0d0d0d",
        "activityBarBadge.background": "#4d8cb6",
        "activityBarBadge.foreground": "#eeeeee",
        "activityBar.border": "#1a1a1a",
        "activityBar.activeBorder": "#444444",
        "activityBar.foreground": "#aaaaaa",
        "activityBar.inactiveForeground": "#333333",
        "breadcrumb.background": "#0d0d0d",
        "breadcrumbPicker.background": "#0e0e0e",
        "contrastBorder": "#1a1a1a",
        "editor.background": "#090909",
        "editor.foreground": "#eeffff",
        "editor.lineHighlightBackground": "#00000000",
        "editor.lineHighlightBorder": "#00000000",
        "editor.selectionHighlightBackground": "#00000000",
        "editor.selectionHighlightBorder": "#b34f16dd",
        "editor.findMatchBackground": "#00000000",
        "editor.findMatchBorder": "#aaff00",
        "editor.findMatchHighlightBackground": "#cc000055",
        "editor.findMatchHighlightBorder": "#cc3300",
