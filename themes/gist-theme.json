        "statusBar.noFolderBorder": "#1a1a1a",
        "statusBarItem.hoverBackground": "#222222",
        "tab.border": "#1a1a1a",
		"tab.hoverBackground": "#1d1d1d",
        "tab.activeBackground": "#252525",
        "tab.activeForeground": "#eeeeee",
        "tab.inactiveBackground": "#0d0d0d",
        "tab.inactiveForeground": "#888888",
        "tab.unfocusedActiveBackground": "#151515",
        "tab.unfocusedActiveBorder": "#1a1a1a",
		"terminal.inactiveSelectionBackground": "#3a3d4180",
        "titleBar.border": "#1d1d1d",
